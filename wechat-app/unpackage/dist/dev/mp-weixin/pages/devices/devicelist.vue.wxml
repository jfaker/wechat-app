<template name="48e9d9e6">
  <view class="_view data-v-b105d2e2 content">
    <view class="_view data-v-b105d2e2 uni-list">
      <view class="_view data-v-b105d2e2 uni-list-cell" hover-class="uni-list-cell-hover" wx:key="key" key="{{key}}" wx:for="{{list}}" wx:for-index="key" wx:for-item="value">
        <view class="_view data-v-b105d2e2 uni-media-list">
          <view class="_view data-v-b105d2e2 uni-media-list-logo">
            <image wx:if="{{showImg}}" src="{{value.img}}" class="_image data-v-b105d2e2"></image>
          </view>
          <view class="_view data-v-b105d2e2 uni-media-list-body">
            <view class="_view data-v-b105d2e2 uni-media-list-text-top">{{value.title}}</view>
            <view class="_view data-v-b105d2e2 uni-media-list-text-bottom uni-ellipsis">{{value.content}}</view>
          </view>
        </view>
      </view>
    </view>
    <view class="_ul data-v-b105d2e2">
      <view wx:for="{{devices}}" wx:for-index="index" wx:for-item="device" class="_li data-v-b105d2e2"> {{device.name}} </view>
    </view>
  </view>
</template>